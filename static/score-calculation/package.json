    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "riot": "<4",
    "riot-route": "^3.1.4",
    "uss": "^0.0.17"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^9.6.0",
