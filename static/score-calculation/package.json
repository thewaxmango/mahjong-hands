    "moduleFileExtensions": [
      "js",
      "coffee"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules",
      "<rootDir>/src"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transformIgnorePatterns": [],
